{
  "name": "xdd",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "scripts/preinstall.sh",
    "watch:main": "next dev",
    "watch:deps": "cd bundledDeps/ui-components && npm run dev --raw",
    "dev": "concurrently npm:watch:main npm:watch:deps",
    "build:main": "next build",
    "build:deps": "cd bundledDeps/ui-components && npm run build",
    "build": "npm run build:deps && npm run build:main",
    "start": "next start"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.19.1",
    "@macrostrat/ui-components": "file:./bundledDeps/ui-components",
    "@mdx-js/loader": "^1.5.1",
    "@next/mdx": "^9.1.3",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-stylus": "^1.0.1",
    "classnames": "^2.2.6",
    "coffeescript": "^2.4.1",
    "next": "^9.3.5",
    "next-coffeescript": "^1.3.0",
    "nextjs-redirect": "^1.0.2",
    "react": "16.10.2",
    "react-dom": "16.10.2",
    "react-hyperscript": "^3.2.0",
    "react-infinite-scroller": "^1.2.4",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "stylus": "^0.54.7"
  },
  "devDependencies": {
    "@types/node": "^13.13.0",
    "@types/react": "^16.9.34",
    "bulk": "^2.0.1",
    "linklocal": "^2.8.2",
    "typescript": "^3.8.3"
  }
}
